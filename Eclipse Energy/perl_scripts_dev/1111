<script language=\"JavaScript1.2\">
var menuNme=''
var ns6=document.getElementById&&!document.all?1:0
var head=\"display:none''\"
var folder=''
window.onload+=preload();
//alert(menuNme);

function preload(){
	if(document.images){
		Open = new Image(10,10)
		Closed = new Image(10,10)
		SubOpen = new Image(10,10)
		SubClosed = new Image(10,10)
		Open.src = \"../image/ico_plus.gif\"
		Closed.src = \"../image/ico_sub.gif\"
		SubOpen.src = \"../image/ico_up.gif\"
		SubClosed.src = \"../image/ico_down.gif\"
	}
	/* Hide All Menu , Show menu : Export Promotion and Privileges */
	if(menuNme!='FreeZone'){
		ns6?expdisplay.nextSibling.nextSibling.style:document.all[expdisplay.sourceIndex-3].style.display = 'none';}
	else{
		expicon.src=Closed.src
	}

	/* Hide All Menu , Show menu : About Us/Administrative */
	if(menuNme!='AboutUs'){
		ns6?transdisplay.nextSibling.nextSibling.style:document.all[transdisplay.sourceIndex-3].style.display = 'none';
	}else{
		transicon.src=Closed.src
	}

	/* Hide All Menu , Show menu : Cargo Clearance */
	if(menuNme!='Cargo'){
		ns6?cargodisplay.nextSibling.nextSibling.style:document.all[cargodisplay.sourceIndex-3].style.display = 'none';
	}else{
		cargoicon.src=Closed.src
	}

	/* Hide All Menu , Show menu : Tariff Nomenclature */
	if(menuNme!='Tariff'){
		ns6?tariffnodisplay.nextSibling.nextSibling.style:document.all[tariffnodisplay.sourceIndex-3].style.display = 'none';
	}else{
		tariffnoicon.src=Closed.src
	}

	/* Hide All Menu , Show menu : Household	Effects */
	if(menuNme!='HouseHold'){
		ns6?hhedisplay.nextSibling.nextSibling.style:document.all[hhedisplay.sourceIndex-3].style.display = 'none';
	}else{
		hheicon.src=Closed.src
	}
	


	/* Hide All Menu , Show menu : Personal Vehicles (Tem) */
	if(menuNme!='PersonalTem' & menuNme!='PersonalPer'){
		ns6?psvdisplay.nextSibling.nextSibling.style:document.all[psvdisplay.sourceIndex-3].style.display = 'none';
		ns6?tempsvdisplay.nextSibling.nextSibling.style:document.all[tempsvdisplay.sourceIndex+1].style.display = 'none';
		ns6?perpsvdisplay.nextSibling.nextSibling.style:document.all[perpsvdisplay.sourceIndex+1].style.display = 'none';
	}else{
		if(menuNme=='PersonalTem'){
			psvicon.src=Closed.src
			tempsvicon.src=SubClosed.src
			ns6?perpsvdisplay.nextSibling.nextSibling.style:document.all[perpsvdisplay.sourceIndex+1].style.display = 'none';
		}else if(menuNme=='PersonalPer'){
			psvicon.src=Closed.src
			perpsvicon.src=SubClosed.src
			ns6?tempsvdisplay.nextSibling.nextSibling.style:document.all[tempsvdisplay.sourceIndex+1].style.display = 'none';
		}
		/* Hide All Menu , Show menu : statisitic */
	if(menuNme!='Statistic'){
		ns6?statdisplay.nextSibling.nextSibling.style:document.all[statdisplay.sourceIndex-3].style.display = 'none';
	}else{
		staticon.src=Closed.src
	}
	}

	/* Hide All Menu , Show menu : Personal Vehicles */
	/*if(menuNme!='PersonalTem'){
		ns6?psvdisplay.nextSibling.nextSibling.style:document.all[psvdisplay.sourceIndex-3].style.display = 'none';
		ns6?tempsvdisplay.nextSibling.nextSibling.style:document.all[tempsvdisplay.sourceIndex+1].style.display = 'none';
	}else{
		psvicon.src=Closed.src
		tempsvicon.src=Closed.src
	}*/

}

function expandit(curobj){
	folder=ns6?curobj.nextSibling.nextSibling.style:document.all[curobj.sourceIndex+1].style
	if (folder.display==\"none\"){	folder.display=\"\"}
	else{	folder.display=\"none\"	}
					//this.style.backgroundColor ='#DCFAF1';

	/* Hide Other Layer (Except : transdisplay) */
	if(curobj.sourceIndex+1==transdisplay.sourceIndex-3){
		ns6?expdisplay.nextSibling.nextSibling.style:document.all[expdisplay.sourceIndex-3].style.display = 'none';
		expicon.src=Open.src
		ns6?cargodisplay.nextSibling.nextSibling.style:document.all[cargodisplay.sourceIndex-3].style.display = 'none';
		cargoicon.src=Open.src
		ns6?tariffnodisplay.nextSibling.nextSibling.style:document.all[tariffnodisplay.sourceIndex-3].style.display = 'none';
		tariffnoicon.src=Open.src
		ns6?hhedisplay.nextSibling.nextSibling.style:document.all[hhedisplay.sourceIndex-3].style.display = 'none';
		hheicon.src=Open.src
		ns6?psvdisplay.nextSibling.nextSibling.style:document.all[psvdisplay.sourceIndex-3].style.display = 'none';
		psvicon.src=Open.src
		ns6?statdisplay.nextSibling.nextSibling.style:document.all[statdisplay.sourceIndex-3].style.display = 'none';
		staticon.src=Open.src
	}
	/* Hide Other Layer (Except : expdisplay) */	
	else if(curobj.sourceIndex+1==expdisplay.sourceIndex-3){
		ns6?transdisplay.nextSibling.nextSibling.style:document.all[transdisplay.sourceIndex-3].style.display = 'none';
		transicon.src=Open.src
		ns6?cargodisplay.nextSibling.nextSibling.style:document.all[cargodisplay.sourceIndex-3].style.display = 'none';
		cargoicon.src=Open.src
		ns6?tariffnodisplay.nextSibling.nextSibling.style:document.all[tariffnodisplay.sourceIndex-3].style.display = 'none';
		tariffnoicon.src=Open.src
		ns6?hhedisplay.nextSibling.nextSibling.style:document.all[hhedisplay.sourceIndex-3].style.display = 'none';
		hheicon.src=Open.src
		ns6?psvdisplay.nextSibling.nextSibling.style:document.all[psvdisplay.sourceIndex-3].style.display = 'none';
		psvicon.src=Open.src
		ns6?statdisplay.nextSibling.nextSibling.style:document.all[statdisplay.sourceIndex-3].style.display = 'none';
		staticon.src=Open.src
	}
	/* Hide Other Layer (Except : cargodisplay) */	
	else if(curobj.sourceIndex+1==cargodisplay.sourceIndex-3){
		ns6?transdisplay.nextSibling.nextSibling.style:document.all[transdisplay.sourceIndex-3].style.display = 'none';
		transicon.src=Open.src
		ns6?tariffnodisplay.nextSibling.nextSibling.style:document.all[tariffnodisplay.sourceIndex-3].style.display = 'none';
		tariffnoicon.src=Open.src
		ns6?expdisplay.nextSibling.nextSibling.style:document.all[expdisplay.sourceIndex-3].style.display = 'none';
		expicon.src=Open.src
		ns6?hhedisplay.nextSibling.nextSibling.style:document.all[hhedisplay.sourceIndex-3].style.display = 'none';
		hheicon.src=Open.src
		ns6?psvdisplay.nextSibling.nextSibling.style:document.all[psvdisplay.sourceIndex-3].style.display = 'none';
		psvicon.src=Open.src
		ns6?statdisplay.nextSibling.nextSibling.style:document.all[statdisplay.sourceIndex-3].style.display = 'none';
		staticon.src=Open.src
	}
	/* Hide Other Layer (Except : tariffnodisplay) */	
	else if(curobj.sourceIndex+1==tariffnodisplay.sourceIndex-3){
		ns6?transdisplay.nextSibling.nextSibling.style:document.all[transdisplay.sourceIndex-3].style.display = 'none';
		transicon.src=Open.src
		ns6?cargodisplay.nextSibling.nextSibling.style:document.all[cargodisplay.sourceIndex-3].style.display = 'none';
		cargoicon.src=Open.src
		ns6?expdisplay.nextSibling.nextSibling.style:document.all[expdisplay.sourceIndex-3].style.display = 'none';
		expicon.src=Open.src
		ns6?hhedisplay.nextSibling.nextSibling.style:document.all[hhedisplay.sourceIndex-3].style.display = 'none';
		hheicon.src=Open.src
		ns6?psvdisplay.nextSibling.nextSibling.style:document.all[psvdisplay.sourceIndex-3].style.display = 'none';
		psvicon.src=Open.src
		ns6?statdisplay.nextSibling.nextSibling.style:document.all[statdisplay.sourceIndex-3].style.display = 'none';
		staticon.src=Open.src
	}
	/* Hide Other Layer (Except : hhedisplay) */	
	else if(curobj.sourceIndex+1==hhedisplay.sourceIndex-3){
		ns6?transdisplay.nextSibling.nextSibling.style:document.all[transdisplay.sourceIndex-3].style.display = 'none';
		transicon.src=Open.src
		ns6?tariffnodisplay.nextSibling.nextSibling.style:document.all[tariffnodisplay.sourceIndex-3].style.display = 'none';
		tariffnoicon.src=Open.src
		ns6?expdisplay.nextSibling.nextSibling.style:document.all[expdisplay.sourceIndex-3].style.display = 'none';
		expicon.src=Open.src
		ns6?cargodisplay.nextSibling.nextSibling.style:document.all[cargodisplay.sourceIndex-3].style.display = 'none';
		cargoicon.src=Open.src
		ns6?psvdisplay.nextSibling.nextSibling.style:document.all[psvdisplay.sourceIndex-3].style.display = 'none';
